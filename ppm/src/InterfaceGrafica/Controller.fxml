<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="526.0" prefWidth="785.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="InterfaceGrafica.Controller">
   <children>
      <ToolBar prefHeight="35.0" prefWidth="785.0">
         <items>
            <Button fx:id="criarAlbum" mnemonicParsing="false" onMousePressed="#carrega_Album" text="Carregar Album" />
            <Button fx:id="carregarImagem" mnemonicParsing="false" onMousePressed="#carrega_Imagem" text="Imagem" />
            <Button fx:id="botao3" mnemonicParsing="false" text="Adicionar Imagem" />
            <Button mnemonicParsing="false" text="Remover Imagem" />
            <Button mnemonicParsing="false" text="Trocar Imagens" />
            <MenuButton fx:id="editar_Imagens" mnemonicParsing="false" onMousePressed="#mostrar" text="MenuButton">
              <items>
                <MenuItem fx:id="adicionar_Imagem" mnemonicParsing="false" text="Adicionar Imagem" />
                <MenuItem fx:id="remover_Imagem" mnemonicParsing="false" onAction="#remover" text="Remover Imagem" />
                  <MenuItem fx:id="trocar_Imagens" mnemonicParsing="false" text="Trocar Imagens" />
              </items>
            </MenuButton>
         </items>
      </ToolBar>
      <ListView fx:id="album" layoutY="38.0" onMouseClicked="#showImage" prefHeight="501.0" prefWidth="144.0" />
      <ToolBar layoutX="144.0" layoutY="35.0" prefHeight="40.0" prefWidth="642.0">
        <items>
          <Button fx:id="rotateR" mnemonicParsing="false" text="Rodar direita" />
            <Button fx:id="rotateL" mnemonicParsing="false" text="Rodar esquerda" />
            <Button fx:id="mirrorV" mnemonicParsing="false" text="MirrorV" textOverrun="CLIP" />
            <Button fx:id="mirrorH" mnemonicParsing="false" text="MirrorH" />
            <Button fx:id="sepia" mnemonicParsing="false" text="Sepia" />
            <Button fx:id="contrast" mnemonicParsing="false" text="Contraste" />
            <Button fx:id="noise" mnemonicParsing="false" text="Noise" />
            <Button fx:id="scale" mnemonicParsing="false" text="Scale" />
        </items>
      </ToolBar>
      <ImageView fx:id="carregarImagem" fitHeight="150.0" fitWidth="200.0" layoutX="278.0" layoutY="171.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
